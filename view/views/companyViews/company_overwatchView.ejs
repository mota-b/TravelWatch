<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    
    <!-- Stylesheets -->
    <%- include('../requireViews/linksView.ejs', {}) %>

    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.5.1/dist/leaflet.css"
    integrity="sha512-xwE/Az9zrjBIphAcBb3F6JVqxf46+CDLwfLMHloNu6KEQCAWi6HcDUbeOfBIptF7tcCzusKFjFw2yuvEpDL9wQ=="
    crossorigin=""/>


  </head>
  <body>
    <%- include('../navBarView.ejs', {}) %>
    
    <!-- Section 1  -->
    <div class="section">

      <!-- lef-list + map -->
      <div class="my-container">
        <div class="row">
          <!-- left-list -->
          <div class="col-3" id="left-list">
            <div id="operator-info">
              
   
              <!-- code start -->
              <div class="twPc-div">
                  <a class="twPc-bg twPc-block"></a>
              
                <div>
                  
                  <a title="title" href="#" class="twPc-avatarLink">
                    <img alt="title" src="/img/profile.png" class="twPc-avatarImg">
                  </a>
              
                  <div class="twPc-divUser">
                    <div class="twPc-divName">
                      <a href="#" id="operator_name">/</a>
                    </div>
                    <!-- <span>
                      <a href="#">@<span>mertskaplan</span></a>
                    </span> -->
                  </div>
              
                  <div class="twPc-divStats">
                    <ul class="twPc-Arrange">
                      <li class="twPc-ArrangeSizeFit">
                        <a href="#" title="9.840 Tweet">
                          <span class="twPc-StatLabel twPc-block" >Company</span>
                          <span class="twPc-StatValue" id="company_name">/</span>
                        </a>
                      </li>
                      <li class="twPc-ArrangeSizeFit">
                        <a href="#" title="885 Following">
                          <span class="twPc-StatLabel twPc-block">C.manager</span>
                          <span class="twPc-StatValue" id="c_manager_name">/</span>
                        </a>
                      </li>
                      <li class="twPc-ArrangeSizeFit">
                        <a href="#" title="1.810 Followers">
                          <span class="twPc-StatLabel twPc-block">Post</span>
                          <span class="twPc-StatValue" id="operator_post">/</span>
                        </a>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
              <!-- code end -->
   
              
              
            </div>
            <div id="entities-list"></div>
            
          </div> <!-- END left-list -->
          
          <!-- main-map -->
          <div class="col-9" id="main-map"></div> <!-- END main-map --> 

        </div>
      </div> <!-- END lef-list + map -->

    </div> <!-- END Section 1  -->
  
    <!-- Section 2  -->
    <div class="section" id="s2">
      hello
    </div> <!-- END Section 1  -->
    
    

     <!-- Make sure you put this AFTER Leaflet's CSS -->
 <script src="https://unpkg.com/leaflet@1.5.1/dist/leaflet.js"
 integrity="sha512-GffPMF3RvMeYyc1LWMHtK8EbPv0iNZ8/oTtHPx9/cc2ILxQ+u905qIwdpULaqDkyBKgOaB57QTMg7ztg8Jm2Og=="
 crossorigin=""></script>

    <!-- Scripts -->
    <%- include('../requireViews/scriptsView.ejs', {}) %>

   
  </body>
</html>
